{{#if controller.displayable }}
  <div class="col-md-12 clearfix border-bottom no-margin">
    <div class='col-md-3'>Education</div>
    <div class='col-md-9'>
      <ul class="list-unstyled">
        {{#each education in controller}}
        {{view App.EducationInListView contentBinding="education"}}
        {{/each}}
      </ul>
    </div>
    {{#if controllers.application.clickable }}
      <div class="btn-group pull-right">
        {{#linkTo "new-education" class="btn"}}<i class="icon-plus-sign"></i> Add education{{/linkTo}}
      </div>
    {{/if}}
  </div>
  <div class="col-md-9">
    {{outlet}}
  </div>
{{/if}}
