{{#if controller.displayable }}
  <div class="col-md-12 clearfix border-bottom no-margin">
    <div class='col-md-3'>Work experience</div>
    <div class='col-md-9'>

      <ul class="list-unstyled">
        {{#each experience in controller}}
        {{view App.ExperienceInListView contentBinding="experience"}}
        {{/each}}
      </ul>
    </div>
    {{#if controllers.application.clickable }}
    <div class="btn-group pull-right">
      {{#linkTo "new-experience" class="btn"}}<i class="icon-plus-sign"></i> Add Experience{{/linkTo}}
    </div>
    {{/if}}
  </div>
  <div class="col-md-9">
    {{outlet}}
  </div>
{{/if}}
