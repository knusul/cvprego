{{#if controller.displayable }}
  {{#view App.FocusView }}
  <div {{bindAttr class=":col-md-12 :clearfix :border-bottom"}}>
    <span class='col-md-3'>Key Skills</span>
    <div class='col-md-6'>
      <ul class="nav nav-list">
        {{#each controller}}
        <li>{{ name }}</li>
        {{/each}}
      </ul>
    </div>
  </div>
  {{/view}}
{{/if}}
